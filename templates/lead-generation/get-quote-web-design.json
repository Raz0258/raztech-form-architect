{
  "template_id": "get-quote-web-design",
  "name": "Get a Quote - Web Design",
  "description": "Detailed quote request form for web design and development projects.",
  "category": "lead-generation",
  "industry": "technology",
  "icon": "ðŸ’»",
  "fields": [
    {
      "type": "text",
      "label": "Full Name",
      "name": "full_name",
      "required": true,
      "placeholder": "John Doe",
      "validation": "text"
    },
    {
      "type": "email",
      "label": "Email Address",
      "name": "email",
      "required": true,
      "placeholder": "john@company.com",
      "validation": "email"
    },
    {
      "type": "phone",
      "label": "Phone Number",
      "name": "phone",
      "required": true,
      "placeholder": "(555) 123-4567",
      "validation": "phone"
    },
    {
      "type": "text",
      "label": "Company/Organization",
      "name": "company",
      "required": false,
      "placeholder": "Your Company Name",
      "validation": "text"
    },
    {
      "type": "text",
      "label": "Current Website (if applicable)",
      "name": "current_website",
      "required": false,
      "placeholder": "https://www.yoursite.com",
      "validation": "url"
    },
    {
      "type": "select",
      "label": "Project Type",
      "name": "project_type",
      "required": true,
      "options": ["New Website", "Website Redesign", "E-commerce Site", "Web Application", "Landing Page", "Not Sure Yet"]
    },
    {
      "type": "checkbox",
      "label": "Features Needed",
      "name": "features",
      "required": false,
      "options": ["Custom Design", "E-commerce", "Blog/CMS", "User Accounts", "Payment Integration", "API Integration", "Mobile App", "SEO Optimization"]
    },
    {
      "type": "select",
      "label": "Estimated Budget",
      "name": "budget",
      "required": true,
      "options": ["Under $5,000", "$5,000 - $10,000", "$10,000 - $25,000", "$25,000 - $50,000", "$50,000+", "Need guidance"]
    },
    {
      "type": "textarea",
      "label": "Project Description",
      "name": "project_description",
      "required": true,
      "placeholder": "Tell us about your project, goals, and any specific requirements...",
      "rows": 5,
      "validation": "text"
    }
  ],
