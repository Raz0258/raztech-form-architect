{
  "template_id": "free-consultation",
  "name": "Free Consultation Request",
  "description": "Lead generation form for offering free consultations or discovery calls.",
  "category": "lead-generation",
  "industry": "general",
  "icon": "ðŸŽ¯",
  "fields": [
    {
      "type": "text",
      "label": "Full Name",
      "name": "full_name",
      "required": true,
      "placeholder": "John Doe",
      "validation": "text"
    },
    {
      "type": "email",
      "label": "Email Address",
      "name": "email",
      "required": true,
      "placeholder": "john@company.com",
      "validation": "email"
    },
    {
      "type": "phone",
      "label": "Phone Number",
      "name": "phone",
      "required": true,
      "placeholder": "(555) 123-4567",
      "validation": "phone"
    },
    {
      "type": "text",
      "label": "Company Name",
      "name": "company",
      "required": false,
      "placeholder": "Your Company",
      "validation": "text"
    },
    {
      "type": "select",
      "label": "What best describes your situation?",
      "name": "situation",
      "required": true,
      "options": ["Need immediate help", "Planning for future", "Exploring options", "Comparing providers", "Other"]
    },
    {
      "type": "textarea",
      "label": "What would you like to discuss?",
      "name": "consultation_topic",
      "required": true,
      "placeholder": "Tell us about your goals and challenges...",
      "rows": 4,
      "validation": "text"
    }
  ],
