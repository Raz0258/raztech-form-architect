{
  "template_id": "service-booking",
  "name": "Service Booking Form",
  "description": "Book appointments and services with preferred date/time selection.",
  "category": "business",
  "industry": "service",
  "icon": "ðŸ“…",
  "fields": [
    {
      "type": "text",
      "label": "Full Name",
      "name": "full_name",
      "required": true,
      "placeholder": "John Doe",
      "validation": "text"
    },
    {
      "type": "email",
      "label": "Email Address",
      "name": "email",
      "required": true,
      "placeholder": "john@example.com",
      "validation": "email"
    },
    {
      "type": "phone",
      "label": "Phone Number",
      "name": "phone",
      "required": true,
      "placeholder": "(555) 123-4567",
      "validation": "phone"
    },
    {
      "type": "select",
      "label": "Service Type",
      "name": "service_type",
      "required": true,
      "options": ["Consultation", "Standard Service", "Premium Service", "Custom Service", "Emergency Service"]
    },
    {
      "type": "text",
      "label": "Preferred Date",
      "name": "preferred_date",
      "required": true,
      "placeholder": "MM/DD/YYYY",
      "validation": "text"
    },
    {
      "type": "select",
      "label": "Preferred Time",
      "name": "preferred_time",
      "required": true,
      "options": ["Morning (9am-12pm)", "Afternoon (12pm-3pm)", "Late Afternoon (3pm-6pm)", "Evening (6pm-9pm)", "Flexible"]
    },
    {
      "type": "text",
      "label": "Alternative Date",
      "name": "alternative_date",
      "required": false,
      "placeholder": "MM/DD/YYYY",
      "validation": "text"
    },
    {
      "type": "select",
      "label": "Location Preference",
      "name": "location",
      "required": false,
      "options": ["Main Office", "Branch Office", "On-site at my location", "Virtual/Remote", "No preference"]
    },
    {
      "type": "textarea",
      "label": "Service Details / Special Requests",
      "name": "service_details",
      "required": false,
      "placeholder": "Please describe your needs or any special requirements...",
      "rows": 4,
      "validation": "text"
    },
    {
      "type": "select",
      "label": "How did you hear about us?",
      "name": "referral_source",
      "required": false,
      "options": ["Google Search", "Social Media", "Friend/Family Referral", "Advertisement", "Existing Customer", "Other"]
    }
  ],
