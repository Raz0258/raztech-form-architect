{
  "template_id": "multi-location-contact",
  "name": "Multi-Location Contact Form",
  "description": "Contact form for businesses with multiple locations or service areas.",
  "category": "contact",
  "industry": "general",
  "icon": "üìç",
  "fields": [
    {
      "type": "text",
      "label": "Full Name",
      "name": "full_name",
      "required": true,
      "placeholder": "John Doe",
      "validation": "text"
    },
    {
      "type": "email",
      "label": "Email Address",
      "name": "email",
      "required": true,
      "placeholder": "john@example.com",
      "validation": "email"
    },
    {
      "type": "phone",
      "label": "Phone Number",
      "name": "phone",
      "required": true,
      "placeholder": "(555) 123-4567",
      "validation": "phone"
    },
    {
      "type": "select",
      "label": "Preferred Location",
      "name": "location",
      "required": true,
      "options": ["New York Office", "Los Angeles Office", "Chicago Office", "Miami Office", "Remote/Virtual"]
    },
    {
      "type": "select",
      "label": "Service Interest",
      "name": "service",
      "required": true,
      "options": ["Consultation", "Service A", "Service B", "Service C", "Multiple Services", "Not Sure"]
    },
    {
      "type": "select",
      "label": "Best Time to Contact",
      "name": "contact_time",
      "required": false,
      "options": ["Morning (9am-12pm)", "Afternoon (12pm-5pm)", "Evening (5pm-8pm)", "Anytime"]
    },
    {
      "type": "text",
      "label": "Subject",
      "name": "subject",
      "required": true,
      "placeholder": "What can we help you with?",
      "validation": "text"
    },
    {
      "type": "textarea",
      "label": "Message",
      "name": "message",
      "required": true,
      "placeholder": "Please provide details about your inquiry...",
      "rows": 5,
      "validation": "text"
    }
  ],
  "sample_data_profiles": {
    "excellent": {
      "full_name": ["Jennifer Martinez", "Robert Johnson", "Amanda Wilson", "Christopher Lee"],
