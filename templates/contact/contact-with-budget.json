{
  "template_id": "contact-with-budget",
  "name": "Contact Form with Budget",
  "description": "Contact form with budget range selector for better lead qualification.",
  "category": "contact",
  "industry": "general",
  "icon": "ðŸ’°",
  "fields": [
    {
      "type": "text",
      "label": "Full Name",
      "name": "full_name",
      "required": true,
      "placeholder": "John Doe",
      "validation": "text"
    },
    {
      "type": "email",
      "label": "Email Address",
      "name": "email",
      "required": true,
      "placeholder": "john@company.com",
      "validation": "email"
    },
    {
      "type": "phone",
      "label": "Phone Number",
      "name": "phone",
      "required": false,
      "placeholder": "(555) 123-4567",
      "validation": "phone"
    },
    {
      "type": "text",
      "label": "Company Name",
      "name": "company",
      "required": false,
      "placeholder": "Your Company Inc.",
      "validation": "text"
    },
    {
      "type": "select",
      "label": "Project Budget",
      "name": "budget",
      "required": true,
      "options": ["Under $5,000", "$5,000 - $10,000", "$10,000 - $25,000", "$25,000 - $50,000", "$50,000+", "Not sure yet"]
    },
    {
      "type": "select",
      "label": "Timeline",
      "name": "timeline",
      "required": false,
      "options": ["Urgent (within 2 weeks)", "Soon (1-2 months)", "Flexible (3+ months)", "Just exploring"]
    },
    {
      "type": "textarea",
      "label": "Project Details",
      "name": "message",
      "required": true,
      "placeholder": "Tell us about your project...",
      "rows": 5,
      "validation": "text"
    }
  ],
  "sample_data_profiles": {
