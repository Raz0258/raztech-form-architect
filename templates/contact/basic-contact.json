{
  "template_id": "basic-contact",
  "name": "Basic Contact Form",
  "description": "Simple, professional contact form with essential fields. Perfect for general inquiries.",
  "category": "contact",
  "industry": "general",
  "icon": "ðŸ“ž",
  "fields": [
    {
      "type": "text",
      "label": "Full Name",
      "name": "full_name",
      "required": true,
      "placeholder": "John Doe",
      "validation": "text"
    },
    {
      "type": "email",
      "label": "Email Address",
      "name": "email",
      "required": true,
      "placeholder": "john@example.com",
      "validation": "email"
    },
    {
      "type": "phone",
      "label": "Phone Number",
      "name": "phone",
      "required": false,
      "placeholder": "(555) 123-4567",
      "validation": "phone"
    },
    {
      "type": "text",
      "label": "Subject",
      "name": "subject",
      "required": true,
      "placeholder": "How can we help you?",
      "validation": "text"
    },
    {
      "type": "textarea",
      "label": "Message",
      "name": "message",
      "required": true,
      "placeholder": "Tell us more about your inquiry...",
      "rows": 5,
      "validation": "text"
    }
  ],
