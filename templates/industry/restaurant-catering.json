{
  "template_id": "restaurant-catering",
  "name": "Catering Request Form",
  "description": "Catering and event food service request form for restaurants.",
  "category": "industry",
  "industry": "hospitality",
  "icon": "üçΩÔ∏è",
  "fields": [
    {
      "type": "text",
      "label": "Full Name",
      "name": "full_name",
      "required": true,
      "placeholder": "John Doe",
      "validation": "text"
    },
    {
      "type": "email",
      "label": "Email Address",
      "name": "email",
      "required": true,
      "placeholder": "john@example.com",
      "validation": "email"
    },
    {
      "type": "phone",
      "label": "Phone Number",
      "name": "phone",
      "required": true,
      "placeholder": "(555) 123-4567",
      "validation": "phone"
    },
    {
      "type": "select",
      "label": "Event Type",
      "name": "event_type",
      "required": true,
      "options": ["Corporate Event", "Wedding", "Birthday Party", "Holiday Party", "Meeting/Conference", "Private Party", "Other"]
    },
    {
      "type": "text",
      "label": "Event Date",
      "name": "event_date",
      "required": true,
      "placeholder": "MM/DD/YYYY",
      "validation": "text"
    },
    {
      "type": "select",
      "label": "Event Time",
      "name": "event_time",
      "required": true,
      "options": ["Breakfast (7am-10am)", "Brunch (10am-1pm)", "Lunch (11am-2pm)", "Afternoon (2pm-5pm)", "Dinner (5pm-9pm)", "Late Night (9pm+)", "All Day"]
    },
    {
      "type": "text",
      "label": "Number of Guests",
      "name": "guest_count",
      "required": true,
      "placeholder": "Approximate number",
      "validation": "text"
    },
    {
      "type": "select",
      "label": "Service Style",
      "name": "service_style",
      "required": true,
      "options": ["Buffet", "Plated Service", "Family Style", "Stations", "Cocktail/Appetizers Only", "Not sure"]
    },
    {
      "type": "checkbox",
      "label": "Dietary Considerations",
      "name": "dietary",
      "required": false,
      "options": ["Vegetarian Options", "Vegan Options", "Gluten-Free", "Dairy-Free", "Nut Allergies", "Halal", "Kosher"]
    },
    {
      "type": "select",
      "label": "Budget per Person",
      "name": "budget",
      "required": false,
      "options": ["Under $20", "$20-$30", "$30-$50", "$50-$75", "$75+", "Need guidance"]
    },
    {
      "type": "text",
      "label": "Event Location/Venue",
      "name": "venue",
      "required": false,
      "placeholder": "Address or venue name",
      "validation": "text"
    },
    {
      "type": "textarea",
      "label": "Additional Details",
      "name": "additional_details",
      "required": false,
      "placeholder": "Menu preferences, special requests, equipment needs, etc...",
      "rows": 4,
      "validation": "text"
    }
  ],
