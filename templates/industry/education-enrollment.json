{
  "template_id": "education-enrollment",
  "name": "Course Enrollment Form",
  "description": "Student enrollment and registration form for educational institutions.",
  "category": "industry",
  "industry": "education",
  "icon": "ðŸŽ“",
  "fields": [
    {
      "type": "text",
      "label": "Student Name",
      "name": "student_name",
      "required": true,
      "placeholder": "John Doe",
      "validation": "text"
    },
    {
      "type": "email",
      "label": "Email Address",
      "name": "email",
      "required": true,
      "placeholder": "john@example.com",
      "validation": "email"
    },
    {
      "type": "phone",
      "label": "Phone Number",
      "name": "phone",
      "required": true,
      "placeholder": "(555) 123-4567",
      "validation": "phone"
    },
    {
      "type": "text",
      "label": "Date of Birth",
      "name": "date_of_birth",
      "required": false,
      "placeholder": "MM/DD/YYYY",
      "validation": "text"
    },
    {
      "type": "select",
      "label": "Program/Course Interest",
      "name": "program",
      "required": true,
      "options": ["Undergraduate Program", "Graduate Program", "Certificate Program", "Professional Development", "Online Course", "Workshop/Seminar", "Not sure yet"]
    },
    {
      "type": "select",
      "label": "Field of Study",
      "name": "field_of_study",
      "required": true,
      "options": ["Business", "Technology/Computer Science", "Healthcare", "Arts & Design", "Education", "Engineering", "Social Sciences", "Other"]
    },
    {
      "type": "select",
      "label": "Enrollment Term",
      "name": "enrollment_term",
      "required": true,
      "options": ["Spring 2026", "Summer 2026", "Fall 2026", "Winter 2026", "ASAP/Rolling", "Exploring options"]
    },
    {
      "type": "select",
      "label": "Attendance Preference",
      "name": "attendance",
      "required": true,
      "options": ["Full-time", "Part-time", "Online only", "Hybrid (online + in-person)", "Flexible"]
    },
    {
      "type": "select",
      "label": "Current Education Level",
      "name": "education_level",
      "required": false,
      "options": ["High School Graduate", "Some College", "Associate's Degree", "Bachelor's Degree", "Master's Degree", "Doctoral Degree", "Other"]
    },
    {
      "type": "checkbox",
      "label": "Interests/Goals",
      "name": "goals",
      "required": false,
      "options": ["Career change", "Career advancement", "Personal enrichment", "Certification/License", "Skill development", "Degree completion"]
    },
    {
      "type": "textarea",
      "label": "Questions or Additional Information",
      "name": "additional_info",
      "required": false,
      "placeholder": "Tell us about your educational goals, questions about programs, etc...",
      "rows": 4,
      "validation": "text"
    }
  ],
