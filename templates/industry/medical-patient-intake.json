{
  "template_id": "medical-patient-intake",
  "name": "Patient Intake Form",
  "description": "Medical practice patient intake and information collection form.",
  "category": "industry",
  "industry": "healthcare",
  "icon": "üè•",
  "fields": [
    {
      "type": "text",
      "label": "Full Name",
      "name": "patient_name",
      "required": true,
      "placeholder": "John Doe",
      "validation": "text"
    },
    {
      "type": "text",
      "label": "Date of Birth",
      "name": "date_of_birth",
      "required": true,
      "placeholder": "MM/DD/YYYY",
      "validation": "text"
    },
    {
      "type": "email",
      "label": "Email Address",
      "name": "email",
      "required": true,
      "placeholder": "john@example.com",
      "validation": "email"
    },
    {
      "type": "phone",
      "label": "Phone Number",
      "name": "phone",
      "required": true,
      "placeholder": "(555) 123-4567",
      "validation": "phone"
    },
    {
      "type": "text",
      "label": "Address",
      "name": "address",
      "required": true,
      "placeholder": "123 Main St, City, State ZIP",
      "validation": "text"
    },
    {
      "type": "text",
      "label": "Emergency Contact Name",
      "name": "emergency_contact",
      "required": true,
      "placeholder": "Jane Doe",
      "validation": "text"
    },
    {
      "type": "phone",
      "label": "Emergency Contact Phone",
      "name": "emergency_phone",
      "required": true,
      "placeholder": "(555) 987-6543",
      "validation": "phone"
    },
    {
      "type": "text",
      "label": "Insurance Provider",
      "name": "insurance_provider",
      "required": false,
      "placeholder": "Blue Cross, United Healthcare, etc.",
      "validation": "text"
    },
    {
      "type": "text",
      "label": "Insurance ID Number",
      "name": "insurance_id",
      "required": false,
      "placeholder": "Policy number",
      "validation": "text"
    },
    {
      "type": "select",
      "label": "Reason for Visit",
      "name": "visit_reason",
      "required": true,
      "options": ["New Patient Consultation", "Annual Check-up", "Specific Concern", "Follow-up Visit", "Preventive Care", "Other"]
    },
    {
      "type": "textarea",
      "label": "Current Symptoms or Concerns",
      "name": "symptoms",
      "required": false,
      "placeholder": "Please describe any current health concerns...",
      "rows": 4,
      "validation": "text"
    }
  ],
