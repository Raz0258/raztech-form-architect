{
  "template_id": "real-estate-inquiry",
  "name": "Property Inquiry Form",
  "description": "Real estate inquiry form for buyers, sellers, and renters.",
  "category": "industry",
  "industry": "real-estate",
  "icon": "üè†",
  "fields": [
    {
      "type": "text",
      "label": "Full Name",
      "name": "full_name",
      "required": true,
      "placeholder": "John Doe"
    },
    {
      "type": "email",
      "label": "Email Address",
      "name": "email",
      "required": true,
      "placeholder": "john@example.com"
    },
    {
      "type": "phone",
      "label": "Phone Number",
      "name": "phone",
      "required": true,
      "placeholder": "(555) 123-4567"
    },
    {
      "type": "select",
      "label": "I'm interested in",
      "name": "interest_type",
      "required": true,
      "options": ["Buying a home", "Selling my property", "Renting a property", "Just exploring"]
    },
    {
      "type": "select",
      "label": "Property Type",
      "name": "property_type",
      "required": true,
      "options": ["Single Family Home", "Condo/Apartment", "Townhouse", "Commercial", "Not sure yet"]
    },
    {
      "type": "select",
      "label": "Budget Range",
      "name": "budget",
      "required": false,
      "options": ["Under $200k", "$200k-$400k", "$400k-$600k", "$600k+", "Not sure yet"]
    },
    {
      "type": "textarea",
      "label": "Additional Details",
      "name": "additional_details",
      "required": false,
      "placeholder": "Tell us more about your needs...",
      "rows": 4
    }
  ],
  "sample_data_profiles": {
